{
  "manifest_version": 3,
  "name": "Easy Open History",
  "version": "1.0.0",
  "description": "Click the button to open the Chrome history page",
  "permissions": ["offscreen"],
  "icons": {
    "16": "history_C7C7C7.png",
    "32": "history_C7C7C7.png",
    "48": "history_C7C7C7.png",
    "128": "history_C7C7C7.png"
  },
  "action": {
    "default_title": "History"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "key": ""
}
